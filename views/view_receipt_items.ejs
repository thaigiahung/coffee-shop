    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Rceipts
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover" id="list-receipt">
                            <thead>
                              <tr>
                                  <th>Mã</th>
                                  <th>Sản phẩm</th>
                                  <th>Số lượng</th>
                                  <th>Đơn giá</th>
                                  <th>Tổng tiền</th>
                              </tr>
                            </thead>
                            <tbody>
                              <% _.each(datas, function (data) { %>
                                <tr>
                                  <td><%= data.id %></td>
                                  <td><%= data.product %></td>
                                  <td><%= data.amount %></td>
                                  <td><%= data.price %></td>
                                  <td><%= data.total %></td>
                                </tr>
                              <% }) %>
                            </tbody>
                        </table>
                    </div>
                    <!-- /.table-responsive -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
    </div>
<script type="text/javascript" charset="utf-8">
    $(document).ready(function() {
        $('#list-receipt').dataTable();
    });
</script>