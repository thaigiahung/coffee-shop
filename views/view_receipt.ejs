    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Rceipts
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover" id="list-receipt">
                            <thead>
                              <tr>
                                  <th>Mã</th>
                                  <th>Khách</th>
                                  <th>Thuế</th>
                                  <th>Giảm giá</th>
                                  <th>Mã giảm giá</th>
                                  <th>Tổng tiền (trước thuế)</th>
                                  <th>Tổng tiền (sau thuế)</th>
                                  <th>Tổng sản phẩm</th>
                                  <th>Cửa hàng</th>
                                  <th>Nhận của khách</th>
                              </tr>
                            </thead>
                            <tbody>
                              <% _.each(datas, function (data) { %>
                                <tr>
                                  <td>
                                    <a href="/view1/<%= data.id %>">
                                      <%= data.id %>
                                    </a>
                                  </td>
                                  <td><%= data.customer %></td>
                                  <td><%= data.tax %></td>
                                  <td><%= data.discount %></td>
                                  <td><%= data.subtotal %></td>
                                  <td><%= data.total %></td>
                                  <td><%= data.total_item %></td>
                                  <td><%= data.time %></td>
                                  <td><%= data.store %></td>
                                  <td><%= data.received_money %></td>
                                </tr>
                              <% }) %>
                            </tbody>
                        </table>
                    </div>
                    <!-- /.table-responsive -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
    </div>
<script type="text/javascript" charset="utf-8">
    $(document).ready(function() {
        $('#list-receipt').dataTable();
    });
</script>